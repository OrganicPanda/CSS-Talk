doctype html
html
  head
    meta(charset='utf-8')
    meta(name='viewport', content='width=device-width, initial-scale=1, maximum-scale=1')
    title V8 UI Workflow

    link(rel='stylesheet', type='text/css', href='build/build.css')
  body
    include ./_mixins.jade

    article
      section(data-bespoke-hash='intro')
        include ./_intro.jade
        
      section(data-bespoke-hash='html')
        include ./_html.jade
        
      section(data-bespoke-hash='css')
        include ./_css.jade
        
      section(data-bespoke-hash='less')
        include ./_less.jade

      section(data-bespoke-hash='style-considerations')
        include ./_style-considerations.jade
      
      section(data-bespoke-hash='our-project')
        include ./_our-project.jade

      section(data-bespoke-hash='our-process')
        include ./_our-process.jade
      
      section(data-bespoke-hash='typical-task')
        include ./_typical-task.jade
      
      section(data-bespoke-hash='typical-pl')
        include ./_typical-pl.jade

    script(src='build/build.js')
